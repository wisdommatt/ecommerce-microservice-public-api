# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User {
  id: ID!
  fullName: String!
  email: String!
  country: String!
}

input Pagination {
  afterId: String
  limit: Int!
}

type LoginResponse {
  jwtToken: String!
  user: User!
}

type Mutation {
  authLogin(email: String!, password: String!): LoginResponse!
}
